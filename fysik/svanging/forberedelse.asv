%% Konstanter
clear

m = 0.1;
k = 40;
dx = 0.014;
omega_sqr = k / m;
x = linspace(0, 1, 100);


%% Uppgift 1
xt = @(t) dx * cos(sqrt(omega_sqr)*t);

plot(x, xt(x))
title('Position mot tid

%% Uppgift 2

vt = diff(xt(x));
vt = [0 vt]; % Eftersom diff bara är numerisk derivata så finns inte 0 med

plot(x, vt)

%% Uppgift 3

plot(xt(x), vt)